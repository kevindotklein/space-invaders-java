import java.awt.Color;
import java.awt.Graphics;

public class Laser {
	private int x, y, velocity;
	public Laser(int x0, int y0) {
		this.x = x0;
		this.y = y0;
		this.velocity = 10;
	}
	
	public void update() {
		
	}
	
	public void render(Graphics g) {
		g.setColor(Color.red);
		g.fillRect(x, y, 10, 20);
	}
}
