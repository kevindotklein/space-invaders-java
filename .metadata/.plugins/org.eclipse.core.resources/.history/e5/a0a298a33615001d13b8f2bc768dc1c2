import java.awt.Color;
import java.awt.Graphics;

public class Alien {
	
	private int x,y,width,height;
	
	public Alien() {
		this.x = MainGame.WIDTH/2;
		this.y = 30;
		this.width = 30;
		this.height = 30;
	}
	
	public void update() {
		
	}
	
	public void render(Graphics g) {
		g.setColor(Color.orange);
		g.fillRect(x, y, width, height);
	}
}
